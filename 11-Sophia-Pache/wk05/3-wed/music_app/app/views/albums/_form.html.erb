<%= form_for @album do |f| %>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :tracks %>
    <%= f.text_field :tracks, :placeholder => '10' %>

    <%= f.label :label%>
    <%= f.text_field :label %>

    <%= f.label :recordedyear%>
    <%= f.text_field :recordedyear, :placeholder => 'YYYY'%>

    <%= f.label :image%>
    <%= f.url_field :image%>

    <%= f.label :artist_id %>
    <%= f.select :artist_id, Artist.pluck(:name, :id), :include_blank => true %>
    <p>

    <%= f.submit %>
<% end %>